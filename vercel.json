{
    "version": 2,
    "buildCommand": "sh vercel.sh",
    "outputDirectory": "frontend/dist",
    "functions": {
      "api/index.js": {
        "maxDuration": 30
      }
    },
    "rewrites": [
        { 
          "source": "/(.*)", 
          "destination": "/api/index.js" 
        }
      ]
}