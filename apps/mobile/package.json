{
  "name": "mobile",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android-dev": "cross-env APP_VARIANT=development expo run:android",
    "android-prev": "cross-env APP_VARIANT=preview expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "android": "expo run:android",
    "check-dependencies": "rnx-align-deps",
    "fix-dependencies": "rnx-align-deps --write"
  },
  "dependencies": {
    "expo": "^53.0.9",
    "expo-build-properties": "~0.14.6",
    "expo-constants": "~17.1.6",
    "expo-linear-gradient": "~14.1.4",
    "expo-linking": "~7.1.5",
    "expo-navigation-bar": "~4.2.4",
    "expo-network": "~7.1.5",
    "expo-router": "~5.0.7",
    "expo-splash-screen": "~0.30.8",
    "expo-status-bar": "~2.2.3",
    "expo-updates": "~0.28.13",
    "react": "19.0.0",
    "react-dom": "^19.0.0",
    "react-native": "^0.79.0",
    "react-native-dropdown-select-list": "^2.0.5",
    "react-native-gesture-handler": "^2.26.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "^4.10.0",
    "react-native-web": "~0.20.0",
    "shared": "*",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native-community/cli": "^18.0.0",
    "@react-native-community/cli-platform-android": "^18.0.0",
    "@react-native-community/cli-platform-ios": "^18.0.0",
    "@react-native/metro-config": "^0.79.0",
    "@rnx-kit/align-deps": "^3.0.6",
    "cross-env": "^7.0.3"
  },
  "private": true,
  "rnx-kit": {
    "kitType": "app",
    "alignDeps": {
      "requirements": [
        "react-native@0.79"
      ],
      "capabilities": [
        "core",
        "core-android",
        "core-ios",
        "gestures",
        "react",
        "react-dom",
        "safe-area",
        "screens"
      ]
    }
  }
}
